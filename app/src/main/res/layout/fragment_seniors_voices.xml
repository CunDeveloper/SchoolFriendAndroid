<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:background="@android:color/white"
    android:layout_height="match_parent" tools:context="com.nju.fragment.SeniorsVoicesFragment">

    <com.nju.View.SchoolFriendAweTextView
        android:layout_width="match_parent"
        android:textSize="20sp"
        android:textStyle="bold"
        android:fontFamily="sans-serif-light"
        android:textColor="@color/colorPrimaryDark"
        android:text="@string/smile"
        android:id="@+id/fragment_seniors_voice_tv"
        android:layout_height="wrap_content" />
    <ProgressBar
        android:layout_width="match_parent"
        style="?android:attr/progressBarStyleHorizontal"
        android:id="@+id/fragment_web_view_progressBar"
        android:max="10000"
        android:progress="40"
        android:layout_height="10dp" />

</FrameLayout>
