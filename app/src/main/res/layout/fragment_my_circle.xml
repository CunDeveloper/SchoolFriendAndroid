                android:layout_height="@dimen/phone_item_height">
                <com.nju.View.SchoolFriendAweTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:text="@string/person_icon"
                    android:id="@+id/tv30"
                    android:layout_marginEnd="@dimen/padding_left"
                    android:textColor="@android:color/holo_green_dark"
                    android:layout_centerVertical="true"
                    android:layout_alignParentStart="true" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="@string/my_heart"
                    android:textColor="@android:color/black"
                    android:layout_alignTop="@+id/tv30"
                    android:layout_toEndOf="@+id/tv30" />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/voice_collect"
                android:layout_width="match_parent"
                android:background="@drawable/item_toggle_bg"
                android:paddingLeft="@dimen/phone_apadding"
                android:layout_height="@dimen/phone_item_height">
