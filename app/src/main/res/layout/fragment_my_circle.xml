<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:background="@color/light_gray"

    android:layout_height="match_parent" tools:context="com.nju.fragment.MyCircleFragment">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@android:color/black"
                android:layout_marginStart="@dimen/phone_apadding"
                android:layout_marginTop="@dimen/phone_apadding"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="@string/alumn_dynamic"
                android:id="@+id/textView24" />

            <RelativeLayout
                android:id="@+id/my_dynamic"
                android:layout_width="match_parent"
                android:background="@drawable/item_toggle_bg"
                android:layout_marginBottom="-1dp"
                android:paddingLeft="@dimen/phone_apadding"
                android:layout_height="@dimen/phone_item_height">

                <com.nju.View.SchoolFriendAweTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:text="@string/person_icon"
                    android:id="@+id/textView28"
                    android:layout_marginEnd="@dimen/padding_left"
                    android:textColor="@android:color/holo_blue_light"
                    android:layout_centerVertical="true"
                    android:layout_alignParentStart="true" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="@string/my_dynamic"
                    android:id="@+id/textView29"
                    android:textColor="@android:color/black"
                    android:layout_alignTop="@+id/textView28"
                    android:layout_toEndOf="@+id/textView28" />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/dynamic_collect"
                android:layout_width="match_parent"
                android:background="@drawable/item_toggle_bg"
                android:paddingLeft="@dimen/phone_apadding"
                android:layout_height="@dimen/phone_item_height">
                <com.nju.View.SchoolFriendAweTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:text="@string/collect_icon1"
                    android:layout_marginEnd="@dimen/padding_left"
                    android:textColor="@android:color/holo_blue_light"
                    android:id="@+id/textView30"
                    android:layout_centerVertical="true"
                    android:layout_alignParentStart="true" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="@string/my_collect"
                    android:textColor="@android:color/black"
                    android:id="@+id/textView31"
                    android:layout_alignBottom="@+id/textView30"
                    android:layout_toEndOf="@+id/textView30" />
            </RelativeLayout>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/phone_apadding"
                android:layout_marginStart="@dimen/phone_apadding"
                android:textColor="@android:color/black"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="@string/xuezhang_heat_voice"
                android:id="@+id/textView25" />
            <RelativeLayout
                android:id="@+id/my_voice"
                android:layout_width="match_parent"
                android:background="@drawable/item_toggle_bg"
                android:layout_marginBottom="-1dp"
