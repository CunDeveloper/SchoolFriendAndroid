                    android:textColor="@android:color/black"
                    android:layout_alignBottom="@+id/tv0"
                    android:layout_toEndOf="@+id/tv0" />
            </RelativeLayout>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/phone_apadding"
                android:layout_marginStart="@dimen/phone_apadding"
                android:textColor="@android:color/black"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="@string/major_ask"
                android:id="@+id/textView26" />
            <RelativeLayout
                android:id="@+id/my_ask"
                android:layout_width="match_parent"
                android:background="@drawable/item_toggle_bg"
                android:layout_marginBottom="-1dp"
                android:paddingLeft="@dimen/phone_apadding"
                android:layout_height="@dimen/phone_item_height">
                <com.nju.View.SchoolFriendAweTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:text="@string/person_icon"
                    android:id="@+id/tv2"
                    android:layout_marginEnd="@dimen/padding_left"
                    android:textColor="@android:color/holo_red_light"
                    android:layout_centerVertical="true"
                    android:layout_alignParentStart="true" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="@string/my_ask"
                    android:textColor="@android:color/black"
                    android:layout_alignTop="@+id/tv2"
                    android:layout_toEndOf="@+id/tv2" />
            </RelativeLayout>
            <RelativeLayout
