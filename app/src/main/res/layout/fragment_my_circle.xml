                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="@string/publsh_ask"
                    android:textColor="@android:color/black"
                    android:layout_alignBottom="@+id/tv4"
                    android:layout_toEndOf="@+id/tv4" />
            </RelativeLayout>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/phone_apadding"
                android:layout_marginStart="@dimen/phone_apadding"
                android:textColor="@android:color/black"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="@string/work_recrict"
                android:id="@+id/textView27" />
            <RelativeLayout
                android:id="@+id/my_recommend"
                android:layout_width="match_parent"
                android:background="@drawable/item_toggle_bg"
                android:layout_marginBottom="-1dp"
                android:paddingLeft="@dimen/phone_apadding"
                android:layout_height="@dimen/phone_item_height">
                <com.nju.View.SchoolFriendAweTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:text="@string/person_icon"
                    android:id="@+id/tv6"
                    android:layout_marginEnd="@dimen/padding_left"
                    android:textColor="@android:color/holo_blue_dark"
                    android:layout_centerVertical="true"
                    android:layout_alignParentStart="true" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="@string/my_recommend"
                    android:textColor="@android:color/black"
                    android:layout_alignTop="@+id/tv6"
                    android:layout_toEndOf="@+id/tv6" />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/recommend_collect"
                android:layout_width="match_parent"
                android:layout_marginBottom="-1dp"
                android:paddingLeft="@dimen/phone_apadding"
                android:background="@drawable/item_toggle_bg"
                android:layout_height="@dimen/phone_item_height">
                <com.nju.View.SchoolFriendAweTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:text="@string/collect_icon1"
                    android:layout_marginEnd="@dimen/padding_left"
                    android:textColor="@android:color/holo_blue_dark"
                    android:id="@+id/tv8"
                    android:layout_centerVertical="true"
                    android:layout_alignParentStart="true" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="@string/my_collect"
                    android:textColor="@android:color/black"
                    android:layout_alignBottom="@+id/tv8"
                    android:layout_toEndOf="@+id/tv8" />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/recommend_publish"
                android:layout_width="match_parent"
                android:background="@drawable/item_toggle_bg"
                android:paddingLeft="@dimen/phone_apadding"
                android:layout_marginBottom="@dimen/phone_item_height"
                android:layout_height="@dimen/phone_item_height">
                <com.nju.View.SchoolFriendAweTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:text="@string/publish_icon"
                    android:layout_marginEnd="@dimen/padding_left"
                    android:textColor="@android:color/holo_blue_dark"
                    android:id="@+id/tv7"
                    android:layout_centerVertical="true"
                    android:layout_alignParentStart="true" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="@string/publish_recommend"
                    android:textColor="@android:color/black"
                    android:layout_alignBottom="@+id/tv7"
                    android:layout_toEndOf="@+id/tv7" />
            </RelativeLayout>

        </LinearLayout>
    </ScrollView>
</FrameLayout>
